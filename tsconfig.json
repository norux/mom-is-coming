{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "declaration": true,
    "pretty": true,
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "noFallthroughCasesInSwitch": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "lib": ["esnext", "dom"]
  }
}
